{
  "name": "smartabp.vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "stylelint": "stylelint \"src/views/codegen/designer/**/*.{css,scss,vue}\" \"src/views/codegen/VisualDesignerView.vue\" \"src/styles/**/*.{css,scss,vue}\"",
    "stylelint:fix": "stylelint \"src/views/codegen/designer/**/*.{css,scss,vue}\" \"src/views/codegen/VisualDesignerView.vue\" \"src/styles/**/*.{css,scss,vue}\" --fix",
    "gen:api": "openapi --input %VITE_API_BASE_URL%/swagger/v1/swagger.json --output src/core/api --client axios",
    "gen:api:ci": "openapi --input $VITE_API_BASE_URL/swagger/v1/swagger.json --output src/core/api --client axios",
    "gen:api:check": "git diff --exit-code src/core/api || (echo API client drift detected && exit 1)",
    "dev": "vite",
    "build": "run-s type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.build.json",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.cjs . --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "codegen": "tsx src/tools/cli.ts generate",
    "codegen:check": "tsx src/tools/cli.ts check",
    "module:add": "tsx src/tools/add-module.ts --input",
    "module:add:codegen": "tsx src/tools/add-module.ts --input --codegen"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,tsx,css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@iconify-json/ep": "^1.2.3",
    "@iconify/vue": "^5.0.0",
    "@microsoft/signalr": "^9.0.6",
    "@vue/compiler-core": "^3.5.13",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.18",
    "echarts": "^6.0.0",
    "element-plus": "^2.8.8",
    "highlight.js": "^11.11.1",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.44.0",
    "pinia": "^3.0.3",
    "source-map": "^0.7.4",
    "uuid": "^9.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^15.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-visualstudio": "^8.40.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "sass": "^1.91.0",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tsx": "^4.6.0",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
